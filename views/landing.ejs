    <body>
    
    <div class="container">
        <% if(error && error.length > 0){ %>
            <div class="alert alert-danger" role="alert">
                <%= error %>
            </div>
        <% } %>
        <% if(success && success.length > 0){ %>
            <div class="alert alert-success" role="alert">
                <%= success %>
            </div>
        <% } %>
    </div>
    
    
        <header id="landing-header">
            <h1>CanvasShare is a place for sharing art.</h1>
            <h2>Create, Share, Discuss, and Donate!</h2>   
             <a class="btn btn-lg btn-landing" href="/gallery">Show me the Art!</a> 
             <a class="btn btn-lg btn-landing" href="/register">Sign me up!</a>
          </header>
    
    
    <ul class="slideshow">
